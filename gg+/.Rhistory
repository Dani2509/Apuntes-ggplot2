library(tidyverse)
library(ggplot2)
library(ggalt)
# Realizando un diagrama de lolliplots
roedores<- msleep %>% filter(order=="Rodentia")
ggplot(roedores, aes(names, sleep_total)) + geom_lollipop() + coord_flip()
ggplot(roedores, aes(names, sleep_total)) + ggalt::geom_lollipop() + coord_flip()
